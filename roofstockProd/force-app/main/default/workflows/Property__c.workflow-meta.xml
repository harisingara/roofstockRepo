    <fieldUpdates>
        <fullName>Artemis_Capex_Reserve</fullName>
        <field>Capex_Reserve__c</field>
        <formula>IF(OR(ISPICKVAL(Region__c, &quot;Atlanta&quot;), ISPICKVAL(Region__c, &quot;Pittsburg&quot;)), 
if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 6), 0.03 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 16), 0.045 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 6), 0.02 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 16),0.035 * Estimated_Annual_Rent__c * (1 - 0.05), 
0.05 * Estimated_Annual_Rent__c * (1 - 0.05))))), 

IF(ISPICKVAL(Region__c, &quot;Indianapolis&quot;), 
if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 6), 0.03 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 16), 0.045 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 6), 0.02 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 16),0.035 * Estimated_Annual_Rent__c * (1 - 0.05), 
0.05 * Estimated_Annual_Rent__c * (1 - 0.05))))), 

if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 6), 0.03 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 90000, 2018 - Year_Built__c &lt; 16), 0.045 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 6), 0.02 * Estimated_Annual_Rent__c * (1 - 0.05), 
if(and(List_Price__c &lt; 150000, 2018 - Year_Built__c &lt; 16),0.035 * Estimated_Annual_Rent__c * (1 - 0.05), 
0.05 * Estimated_Annual_Rent__c * (1 - 0.05)))) 
)))</formula>
        <name>Artemis / Capex Reserve</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Artemis_Insurance</fullName>
        <field>Insurance__c</field>
        <formula>if(isblank( Insurance_Portal_Quote__c ), 
IF(ISPICKVAL(Region__c, &quot;Atlanta&quot;), 120 + (Square_Feet__c*0.75 + Estimated_Rent__c*0.12)*0.40 + if(Is_in_Flood_Zone__c = True, 0.01 * List_Price__c, 0), 
IF(ISPICKVAL(Region__c, &quot;Indianapolis&quot;), Square_Feet__c * 0.75 * 0.45 + 0.01* List_Price__c, 
IF(ISPICKVAL(Region__c, &quot;Pittsburg&quot;), 120 + ( Estimated_Resale__c /100 + Estimated_Annual_Rent__c /100)*0.40 + if(Is_in_Flood_Zone__c = True, 0.01* List_Price__c, 0), 
120 + (Square_Feet__c*0.75 + Estimated_Rent__c*0.12)*0.40 + if(Is_in_Flood_Zone__c = True, 0.01 * List_Price__c, 0) 
))),Insurance_Portal_Quote__c )</formula>
        <name>Artemis / Insurance</name>
