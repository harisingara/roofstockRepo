    <fieldUpdates>
        <fullName>Roofstock_Leasing_Fees</fullName>
        <field>Leasing_Fee__c</field>
        <formula>1.5 * Estimated_Rent__c / 5</formula>
        <name>Roofstock / Leasing Fees</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Roofstock_Max_ARV</fullName>
        <field>Max_ARV__c</field>
        <formula>IF(Cap_Rate_at_List_Price__c &gt; 0.08, 1.03,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.08, Cap_Rate_at_List_Price__c &gt; 0.07), 1.02,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.07, Cap_Rate_at_List_Price__c &gt; 0.06), 1,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.06, Cap_Rate_at_List_Price__c &gt; 0.05), 1,
IF(Cap_Rate_at_List_Price__c &lt; 0.05, 1, null)))))</formula>
        <name>Roofstock / Max % ARV</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Roofstock_Max_Bid</fullName>
        <field>Max_Bid__c</field>
        <formula>( (Estimated_Sale_Price__c - Estimated_Disposition_Cost__c)/(1+  Required_Margin__c )  ) -  IF(NOT(ISBLANK(Estimated_Capex_Manual__c)), Estimated_Capex_Manual__c, Estimated_Capex__c) -  Estimated_Holding_Cost__c -  Estimated_Acquisition_Cost__c</formula>
        <name>Roofstock / Max Bid</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>Roofstock_Min_ARV</fullName>
        <field>Min_ARV__c</field>
        <formula>IF(Cap_Rate_at_List_Price__c &gt; 0.08, 0.9,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.08, Cap_Rate_at_List_Price__c &gt; 0.07), 0.9,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.07, Cap_Rate_at_List_Price__c &gt; 0.06), 0.9,
IF(AND(Cap_Rate_at_List_Price__c &lt;= 0.06, Cap_Rate_at_List_Price__c &gt; 0.05), 0.9,
IF(Cap_Rate_at_List_Price__c &lt; 0.05, 0.9, null)))))</formula>
        <name>Roofstock / Min % ARV</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
