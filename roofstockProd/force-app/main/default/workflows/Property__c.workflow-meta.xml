    <fieldUpdates>
        <fullName>Roofstock_Neighborhood_Base_Cap_Rate</fullName>
        <field>Base_Cap_Rate_at_Exit__c</field>
        <formula>IF( ISPICKVAL( Neighborhood_Score__c, &quot;1&quot;), 0.09,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;1.5&quot;), 0.085,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;2&quot;), 0.08,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;2.5&quot;), 0.0725,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;3&quot;), 0.07,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;3.5&quot;), 0.065,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;4&quot;), 0.06,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;4.5&quot;), 0.06,
IF( ISPICKVAL( Neighborhood_Score__c, &quot;5&quot;), 0.05, null)))))))))</formula>
        <name>Roofstock / Neighborhood Base Cap Rate</name>
