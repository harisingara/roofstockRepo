    <rules>
        <fullName>Marketing Ready</fullName>
        <actions>
            <name>Rental_Listing_Email_2_Marketing_Ready</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Rental_Listing__c.Marketing_Ready__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Send an Email alert to PM when Rental listing is marked as marketing ready.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>PM Ready</fullName>
        <actions>
            <name>Rental_Listing_Email_3_PM_Ready</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Rental_Listing__c.PM_Ready__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>Send an Email alert to team when Rental listing is marked as PM ready.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Rental Listing Created Marketing Notification</fullName>
        <actions>
            <name>Rental_Listing_Email_1_Prepare_the_listing</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Update_PM_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_RM_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>When Rental Listing is created send email to team.</description>
        <formula>1=1</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Set Existing Photos True for Project Trek</fullName>
        <actions>
            <name>Set_Existing_Photos_to_True</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>Property_Unit__r.Property__r.Client__c == &apos;Empire&apos;</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Set Rently Photo Uploaded date</fullName>
        <actions>
            <name>Set_Photos_Uploaded_to_Rently_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Rental_Listing__c.Photos_Uploaded_to_Rently__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Rental_Listing__c.Photos_Uploaded_to_Rently_Date__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Stamp First Auto Showing Date</fullName>
        <actions>
            <name>Stamp_First_Auto_Showing_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Populate First auto showing date</description>
        <formula>AND( 
 ISCHANGED( Auto_Showing_Date__c ),
 NOT(ISBLANK(Auto_Showing_Date__c )),
 ISBLANK( First_Auto_Showing_Date__c )
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Stamp Leased Date</fullName>
        <actions>
            <name>Stamp_Leased_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Stamp Leased Date when Rently Status is changed to Leased</description>
        <formula>AND(
   ISCHANGED( Status__c ),
   ISPICKVAL(Status__c, &apos;Leased&apos;)
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>
